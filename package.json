{
    "name": "apic-devportal",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build",
        "ts": "tsc --noEmit",
        "eslint:base": "eslint . --ext .ts,.tsx --ignore-path ./.gitignore",
        "eslint:check": "yarn eslint:base --max-warnings 0",
        "eslint:fix": "yarn eslint:base --fix",
        "prettier:base": "prettier --log-level warn \"./**/*.{ts,tsx,css,md,json}\"",
        "prettier:check": "yarn prettier:base --check",
        "prettier:fix": "yarn prettier:base --write",
        "lint:check": "yarn eslint:check && yarn prettier:check",
        "lint": "yarn eslint:fix",
        "preview": "vite preview"
    },
    "dependencies": {
        "@azure/msal-browser": "^3.13.0",
        "@fluentui/react-components": "^9.47.5",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^8.0.7",
        "react-router-dom": "6.22.3",
        "rehype-raw": "^7.0.0",
        "rehype-truncate": "^1.2.2",
        "remark-gfm": "^3.0.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.78",
        "@types/react-dom": "^18.2.25",
        "@typescript-eslint/eslint-plugin": "^7.6.0",
        "@typescript-eslint/parser": "^7.6.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^12.1.0",
        "prettier": "^3.2.5",
        "sass": "^1.75.0",
        "typescript": "^5.4.5",
        "vite": "5.4.6"
    },
    "engines": {
        "node": ">=18"
    }
}
